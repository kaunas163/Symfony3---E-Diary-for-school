<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="EDiaryBundle\Entity\Roles" table="roles">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="title" type="string" column="title" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="description" type="string" column="description" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="pages" type="string" column="pages" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="courses" type="string" column="courses" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="lessons" type="string" column="lessons" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
